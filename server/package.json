{
  "name": "deepa_web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./app/www --presets es2015,stage-2",
    "nodemon": "nodemon ./app/www --exec babel-node",
    "build": "rm -rf dist && mkdir dist && babel app -d dist --copy-files",
    "doc": "apidoc -i app/routes -o public/doc/",
    "tests": "mocha --compilers js:babel-core/register ./app/**/__tests__/**/*.js",
    "test": "mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "colors": "^1.1.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "joi": "^10.4.1",
    "mongoose": "^4.9.4",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0"
  }
}
